{
  "name": "landing-page",
  "version": "0.0.1",
  "description": "A simple personal landing page",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "emotion",
    "hmr"
  ],
  "author": "Chris Barry",
  "license": "MIT",
  "repository": {
    "type": "git"

  },
  "bugs": {

  },

  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-serve ./configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-emotion": "^9.2.6",
    "csstype": "^2.5.6",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.11.0",
    "tunnel-agent": "^0.6.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "emotion": "^9.2.6",
    "react-emotion": "^9.2.6"
  },
  "postcss": {}
}
